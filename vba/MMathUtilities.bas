'@IgnoreModule UseMeaningfulName
Option Explicit

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Convert angle argument to radians."
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Function ConvertToRadians(ByVal angle As Double) As Double
    ConvertToRadians = angle * dPi / 180
End Function

