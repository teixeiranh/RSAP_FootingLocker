VERSION 1.0 CLASS
BEGIN
  MultiUse = -1  'True
END
Attribute VB_Name = "CSlabFootingPoints"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = False
Attribute VB_Exposed = False
Attribute VB_Description = "Data transfer class that enables the tranfer of data between procedures easily."
'@Folder "RSAPFootings.03_FoundationSlab"
'@IgnoreModule VariableNotUsed
'@IgnoreModule ProcedureNotUsed
'//////////////////////////////////////////////////////////////////////////////////////////////////
'@ModuleDescription "Class for the coordinates of a foundation footing."
'
'Developer: Nuno Teixeira
'Email: teixeiranh@gmail.com
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Option Explicit

'@VariableDescription "Auxiliary value to be used in method to determine the value of coordinates."
Private Const HIGHEST_POINT As Double = 2147483647
'@VariableDescription "Auxiliary value to be used in method to determine the value of coordinates."
Private Const LOWEST_POINT As Double = -2147483648#

'@VariableDescription "Z coordinate for the foundation slab."
Private zCoordinate_ As Double

'@VariableDescription "X1 coordinate for the foundation slab."
Private x1Coordinate_ As Double

'@VariableDescription "X2 coordinate for the foundation slab."
Private x2Coordinate_ As Double

'@VariableDescription "Y1 coordinate for the foundation slab."
Private y1Coordinate_ As Double

'@VariableDescription "Y2 coordinate for the foundation slab."
Private y2Coordinate_ As Double

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Constructor that sets the initial values."
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Private Sub Class_Initialize()
    zCoordinate_ = HIGHEST_POINT
    x1Coordinate_ = HIGHEST_POINT
    x2Coordinate_ = LOWEST_POINT
    y1Coordinate_ = HIGHEST_POINT
    y2Coordinate_ = LOWEST_POINT
End Sub

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Getter for the zCoordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Get zCoordinate() As Double
    zCoordinate = zCoordinate_
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Getter for the x1Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Get x1Coordinate() As Double
    x1Coordinate = x1Coordinate_
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Getter for the x2Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Get x2Coordinate() As Double
    x2Coordinate = x2Coordinate_
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Getter for the y1Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Get y1Coordinate() As Double
    y1Coordinate = y1Coordinate_
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Getter for the y2Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Get y2Coordinate() As Double
    y2Coordinate = y2Coordinate_
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Setter for the zCoordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Let zCoordinate(ByVal value As Double)
    zCoordinate_ = value
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Setter for the x1Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Let x1Coordinate(ByVal value As Double)
    x1Coordinate_ = value
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Setter for the x2Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Let x2Coordinate(ByVal value As Double)
    x2Coordinate_ = value
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Setter for the y1Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Let y1Coordinate(ByVal value As Double)
    y1Coordinate_ = value
End Property

'//////////////////////////////////////////////////////////////////////////////////////////////////
'@Description "Setter for the y2Coordinate"
'//////////////////////////////////////////////////////////////////////////////////////////////////
'
Public Property Let y2Coordinate(ByVal value As Double)
    y2Coordinate_ = value
End Property
